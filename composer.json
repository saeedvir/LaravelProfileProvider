{
    "name": "saeedvir/laravel-profile-provider",
    "description": "Laravel package to profile and analyze service provider performance with timing, memory usage and diagnostic analysis",
    "keywords": [
        "laravel",
        "profile",
        "service provider",
        "performance",
        "optimization"
    ],
    "homepage": "https://github.com/saeedvir/LaravelProfileProvider",
    "license": "MIT",
    "authors": [
        {
            "name": "Saeed Abdollahian",
            "email": "saeed.es91@gmail.com",
            "homepage": "https://github.com/saeedvir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Saeedvir\\LaravelProfileProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saeedvir\\LaravelProfileProvider\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saeedvir\\LaravelProfileProvider\\LaravelProfileProviderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}